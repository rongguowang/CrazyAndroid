<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
<Button android:id="@+id/textview"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/text_view_button_text"/>    
<Button android:id="@+id/login"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/login_button_text"/>
<Button android:id="@+id/imagebutton"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/image_button_text"/>
<Button android:id="@+id/patch9_button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/patch9_button_text"/>
<Button android:id="@+id/radio_check_button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/radio_check_button_text" />
</LinearLayout>